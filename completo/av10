from random import randint

SURPRESINHA = []
MEGASENA = []
acertos = 0
a = 0

for i in range (6):

    nu = randint(1,60)
    if (nu not in SURPRESINHA):
        SURPRESINHA.append(nu)
for i in range(6):

    for j in range(6):

        if(SURPRESINHA[i] <= SURPRESINHA[j]):
            a = SURPRESINHA[j]
            SURPRESINHA[j] = SURPRESINHA[i]
            SURPRESINHA[i] = a


print(SURPRESINHA)

for i in range (6):

    MEGASENA.append(int(input(f" {i+1}/6 digite um numero: ")))

for i in range (6):
    if (MEGASENA[i] in SURPRESINHA):
            acertos += 1

print(f"vocÃª acertou {acertos} numeros")